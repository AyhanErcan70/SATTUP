  <property name="styleSheet">
   <string notr="true">/* --- GENEL AYARLAR --- */
QWidget {
    font-family: &quot;Segoe UI&quot;, &quot;Calibri&quot;, sans-serif;
    font-size: 14px;
    color: #333333;
}

/* --- BUTONLAR --- */
QPushButton {
    background-color: #2980b9; /* Kurumsal Mavi */
    color: white;
    border-radius: 5px;
    padding: 8px 15px;
    font-weight: bold;
    border: none;
}
QPushButton:hover {
    background-color: #3498db; /* Üzerine gelince açıl */
}
QPushButton:pressed {
    background-color: #1f618d; /* Basınca koyulaş */
}
/* &quot;SİL&quot; Butonlarını Kırmızı Yapmak İstersen Özel İsimle Yakala */
QPushButton#btn_sil, QPushButton#btn_satir_sil {
    background-color: #c0392b;
}
QPushButton#btn_sil:hover, QPushButton#btn_satir_sil:hover {
    background-color: #e74c3c;
}

/* --- GİRİŞ KUTULARI (LineEdit, DateEdit, ComboBox) --- */
QLineEdit, QComboBox, QDateEdit, QTimeEdit {
    border: 1px solid #bdc3c7;
    border-radius: 4px;
    padding: 5px;
    background-color: white;
}
/* İçine tıklayınca (Focus) çerçeve parlasın */
QLineEdit:focus, QComboBox:focus, QDateEdit:focus {
    border: 2px solid #3498db;
}

/* --- TABLOLAR (En Önemlisi!) --- */
QTableWidget {
    background-color: white;
    gridline-color: #ecf0f1; /* Hücre çizgileri silik gri */
    border: 1px solid #bdc3c7;
}
/* Başlık Satırı */
QHeaderView::section {
    background-color: #2c3e50; /* Koyu Lacivert Başlık */
    color: white;
    padding: 5px;
    border: none;
    font-weight: bold;
}
/* Seçili Satır */
QTableWidget::item:selected {
    background-color: #d6eaf8; /* Açık mavi seçim */
    color: black;
}

/* --- GROUP BOX (Çerçeveler) --- */
QGroupBox {
    border: 2px solid #bdc3c7;
    border-radius: 6px;
    margin-top: 20px; /* Başlık için yer aç */
    font-weight: bold;
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center; /* Başlığı ortala */
    padding: 0 10px;
    color: #2c3e50;
}</string>
