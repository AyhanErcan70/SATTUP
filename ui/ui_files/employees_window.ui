  <property name="styleSheet">
   <string notr="true">/* Input Alanları (QLineEdit, QDateEdit, QComboBox) */
QLineEdit, QDateEdit, QComboBox {
    border: 1px solid #c0c0c0;
    border-radius: 4px;
    padding: 5px;
    background-color: #ffffff;
    selection-background-color: #3d5afe;
    color: #333333;
}

QLineEdit:focus, QDateEdit:focus, QComboBox:focus {
    border: 2px solid #3d5afe;
    background-color: #f0f4ff;
}

/* Kaydet ve İşlem Butonları */
QPushButton {
    background-color: #2c3e50;
    color: white;
    border-radius: 4px;
    padding: 8px 15px;
    font-weight: bold;
    min-height: 18px;
}

QPushButton:hover {
    background-color: #34495e;
    border: 1px solid #3d5afe;
}

QPushButton:pressed {
    background-color: #1a252f;
    padding-top: 10px; /* Tıklama hissi verir */
}

/* Sil Butonu (Özel Renk) */
QPushButton#btn_sil {
    background-color: #c0392b;
}

QPushButton#btn_sil:hover {
    background-color: #e74c3c;
}

/* Temizle/Yeni Kayıt Butonu (Özel Renk) */
QPushButton#btn_yeni, QPushButton#btn_temizle_f {
    background-color: #27ae60;
}

QPushButton#btn_yeni:hover, QPushButton#btn_temizle_f:hover {
    background-color: #2ecc71;
}

/* Tablo Görünümü */
QTableWidget {
    gridline-color: #dcdde1;
    border: 1px solid #c0c0c0;
    selection-background-color: #3d5afe;
    selection-color: white;
}

QHeaderView::section {
    background-color: #f8f9fa;
    padding: 4px;
    border: 1px solid #dcdde1;
    font-weight: bold;
}</string>
